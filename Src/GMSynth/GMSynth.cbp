					<Add option="-O3" />
					<Add option="-g" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-DUNIX" />
					<Add option="-DDEBUG" />
					<Add option="-DUSE_TINYXML" />
					<Add option="-DGCC" />
					<Add directory="$(#BSHOME)\Include" />
					<Add directory="$(#BSHOME)\Src\Instruments" />
				</Compiler>
				<Linker>
					<Add library="$(#BSHOME)\Lib\Instruments.a" />
					<Add library="$(#BSHOME)\Lib\Common.a" />
