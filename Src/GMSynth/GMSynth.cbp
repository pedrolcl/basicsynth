				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-W" />
					<Add option="-O0" />
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D_WINDOWS" />
					<Add option="-D_USRDLL" />
					<Add option="-DGMSYNTH_EXPORTS" />
					<Add directory="$(#BSHOME)\Include" />
					<Add directory="$(#BSHOME)\Src\Instruments" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#BSHOME)\Include" />
					<Add directory="$(#BSHOME)\Src\Instruments" />
				</ResourceCompiler>
				<Linker>
					<Add library="$(#BSHOME)\Lib\InstrumentsD.a" />
