<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="GMSynth" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug Win32">
				<Option platforms="Windows;" />
				<Option output="$(#BSHOME)/Bin/GMSynthD" prefix_auto="0" extension_auto="1" />
				<Option object_output="gccDebug" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-W" />
					<Add option="-O0" />
					<Add option="-mthreads" />
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D_WINDOWS" />
					<Add option="-D_USRDLL" />
					<Add option="-DGMSYNTH_EXPORTS" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#BSHOME)/Include" />
					<Add directory="$(#BSHOME)/Src/Instruments" />
				</ResourceCompiler>
				<Linker>
					<Add library="InstrumentsD" />
					<Add library="CommonD" />
					<Add library="winmm" />
				</Linker>
			</Target>
			<Target title="Release Win32">
				<Option platforms="Windows;" />
				<Option output="$(#BSHOME)/Bin/GMSynth" prefix_auto="1" extension_auto="1" />
				<Option object_output="gccRelease" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-W" />
					<Add option="-mthreads" />
					<Add option="-DWIN32" />
					<Add option="-DNDEBUG" />
					<Add option="-D_WINDOWS" />
					<Add option="-D_USRDLL" />
					<Add option="-DGMSYNTH_EXPORTS" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#BSHOME)/Include" />
					<Add directory="$(#BSHOME)/Src/Instruments" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="Instruments" />
					<Add library="Common" />
					<Add library="winmm" />
				</Linker>
			</Target>
			<Target title="Debug UNIX">
				<Option platforms="Unix;" />
				<Option output="$(#BSHOME)/Lib/libGMSynthD" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#BSHOME)/Bin" />
				<Option object_output="gccDebug" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-g" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-pthread" />
					<Add option="-DUNIX" />
					<Add option="-DDEBUG" />
					<Add option="-DUSE_TINYXML" />
					<Add option="-DGCC" />
				</Compiler>
				<Linker>
					<Add library="InstrumentsD" />
					<Add library="CommonD" />
					<Add library="asound" />
				</Linker>
			</Target>
			<Target title="Release UNIX">
				<Option platforms="Unix;" />
				<Option output="$(#BSHOME)/Lib/libGMSynth" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#BSHOME)/Bin" />
				<Option object_output="gccDebug" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-pthread" />
					<Add option="-DUNIX" />
					<Add option="-DNDEBUG" />
					<Add option="-DUSE_TINYXML" />
					<Add option="-DGCC" />
				</Compiler>
				<Linker>
					<Add library="Instruments" />
					<Add library="Common" />
					<Add library="asound" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="$(#BSHOME)/Include" />
			<Add directory="$(#BSHOME)/Src/Instruments" />
		</Compiler>
		<Linker>
			<Add directory="$(#BSHOME)/Lib" />
		</Linker>
		<Unit filename="GMInstrManager.h" />
		<Unit filename="GMSynth.cpp" />
		<Unit filename="GMSynthDLL.h" />
		<Unit filename="ReadMe.txt" />
		<Unit filename="dllmain.cpp">
			<Option target="Debug Win32" />
			<Option target="Release Win32" />
		</Unit>
		<Unit filename="stdafx.cpp" />
		<Unit filename="stdafx.h">
			<Option compile="1" />
		</Unit>
		<Unit filename="targetver.h" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
