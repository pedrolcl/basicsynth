					<Add option="-O2" />
					<Add option="-W" />
					<Add option="-DWIN32" />
					<Add option="-DNDEBUG" />
					<Add option="-D_WINDOWS" />
					<Add option="-D_USRDLL" />
					<Add option="-DGMSYNTH_EXPORTS" />
					<Add directory="$(#BSHOME)\Include" />
					<Add directory="$(#BSHOME)\Src\Instruments" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#BSHOME)\Include" />
					<Add directory="$(#BSHOME)\Src\Instruments" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="$(#BSHOME)\Lib\Instruments.a" />
					<Add library="$(#BSHOME)\Lib\Common.a" />
